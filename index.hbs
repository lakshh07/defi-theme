<main>
      <section>
        <div class="box-1">
          <div class="box-1-part">
            <div class="box-1-p">
              <img src={{asset "images/main-site/aave.png"}} alt="" />
              <div>
                <h1 class="box-1-heading">defi masterclass</h1>
                <p class="box-1-para">your guide to web3 defi</p>
              </div>
            </div>
          </div>
        </div>

        <div class="box-2">
          <div class="box-2-part">
            <img src={{asset "images/main-site/IMG_0039.png"}} alt="" />
            <button class="box-2-btn">Book an appointment</button>
          </div>
          <div class="box-2-part">
            <img src={{asset "images/main-site/IMG_0040.png"}} alt="" />
            <button class="box-2-btn">Sign up for our course</button>
          </div>
          <div class="box-2-part b">
            <img src={{asset "images/main-site/IMG_0041.png"}} alt="" />
            <button class="box-2-btn">Subscribe to my youtube channel</button>
          </div>
        </div>
      </section>

      <section>
        <div class="box-3">
          <h1 class="box-3-heading heading">this week in defi masterClass</h1>
          <div class="box-3-part cards">
            {{#get "posts" limit="2" filter="tags:[new]" as |new_posts| }}
              {{#foreach new_posts}}
                <a href="{{url}}" target="_blank" rel="noopener noreferrer">
                  <div class="cards__item">
                    <div class="box-3-card">
                      <img
                        src={{img_url feature_image}}
                        alt=""
                      />
                      <h2>{{title}}</h2>
                      <p>
                        {{excerpt}}
                        {{!-- <span>read-more</span> --}}
                      </p>
                    </div>
                  </div>
                </a>
              {{/foreach}}
            {{/get}}         
            </div>
          </div>
        </div>
      </section>

